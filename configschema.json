{
	"$schema": "http://json-schema.org/draft-07/schema",

	"type": "object",
	"properties": {
		"twitter": {
			"type": "object",
			"properties": {
				"consumerKey": {
					"type": "string"
				},
				"consumerSecret": {
					"type": "string"
				},
				"accessToken": {
					"type": "string"
				},
				"accessTokenSecret": {
					"type": "string"
				},
				"debug": {
					"type": "boolean"
				}
			},
			"required": [
				"consumerKey",
				"consumerSecret",
				"accessToken",
				"accessTokenSecret"
			],
			"additionalProperties": false
		},
		"google": {
			"type": "object",
			"properties": {
				"apiKey": {
					"type": "string"
				},
				"spreadsheetId": {
					"type": "string"
				}
			},
			"required": ["apiKey", "spreadsheetId"],
			"additionalProperties": false
		}
	},
	"additionalProperties": false
}
